{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Server=localhost;Database=ContactManager;Trusted_Connection=True;"
    }
  },
  "EntityFramework": {
    "BillingContext": {
      "ConnectionStringKey": "Data:DefaultConnection:ConnectionString",
      "pathFormat": "/logs/ContactManager_Sql",
      "logQueryEnabled": "false"
    }
  }
}
